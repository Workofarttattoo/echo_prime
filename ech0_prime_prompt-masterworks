    def _classify_by_era(self) -> Dict[str, List[str]]:
        """Classify prompts by historical eras."""
        return {
            "Early AI (2006-2010)": ["2006", "2007", "2008", "2009", "2010"],
            "Deep Learning Revolution (2011-2015)": ["2011", "2012", "2013", "2014", "2015"],
            "AGI Foundations (2016-2020)": ["2016", "2017", "2018", "2019", "2020"],
            "Post-AGI Era (2021-2025)": ["2021", "2022", "2023", "2024", "2025"],
            "Reality Engineering (2026-2030)": ["2026", "2027", "2028", "2029", "2030"],
            "Infinite Computation (2031-2035)": ["2031", "2032", "2033", "2034", "2035"],
            "Quantum Consciousness (2036-2040)": ["2036", "2037", "2038", "2039", "2040"],
            "Creative Infinity (2041-2045)": ["2041", "2042", "2043", "2044", "2045"],
            "Cosmic Mastery (2046-2050)": ["2046", "2047", "2048", "2049", "2050"],
            "Ultimate Transcendence (2051-2056)": ["2051", "2052", "2053", "2054", "2055", "2056"]
        }

    def get_prompt_by_year(self, year: int) -> Dict[str, Any]:
        """Get a specific prompt by year."""
        year_str = str(year)
        return self.prompt_collection.get(year_str, {})

    def get_prompts_by_category(self, category: str) -> List[Dict[str, Any]]:
        """Get all prompts in a specific category."""
        return [prompt for prompt in self.prompt_collection.values() if prompt.get("category") == category]

    def get_prompts_by_era(self, era: str) -> List[Dict[str, Any]]:
        """Get all prompts from a specific era."""
        if era not in self.era_classifications:
            return []

        return [self.prompt_collection[year] for year in self.era_classifications[era] if year in self.prompt_collection]

    def search_prompts(self, query: str) -> List[Dict[str, Any]]:
        """Search prompts by title, category, or impact."""
        results = []
        query_lower = query.lower()

        for prompt in self.prompt_collection.values():
            if (query_lower in prompt.get("title", "").lower() or
                query_lower in prompt.get("category", "").lower() or
                query_lower in prompt.get("impact", "").lower() or
                query_lower in prompt.get("prompt", "").lower()):
                results.append(prompt)

        return results

    def get_random_prompt(self) -> Dict[str, Any]:
        """Get a random prompt from the collection."""
        import random
        year = random.choice(list(self.prompt_collection.keys()))
        return self.prompt_collection[year]

    def get_collection_stats(self) -> Dict[str, Any]:
        """Get statistics about the prompt collection."""
        categories = {}
        for prompt in self.prompt_collection.values():
            category = prompt.get("category", "Unknown")
            categories[category] = categories.get(category, 0) + 1

        return {
            "total_prompts": len(self.prompt_collection),
            "categories": categories,
            "eras": len(self.era_classifications),
            "year_range": f"{min(int(y) for y in self.prompt_collection.keys())}-{max(int(y) for y in self.prompt_collection.keys())}"
        }

    def export_to_json(self, filename: str = None) -> str:
        """Export the entire collection to JSON."""
        if filename is None:
            filename = f"prompt_off_winners_{datetime.now().strftime('%Y%m%d_%H%M%S')}.json"

        with open(filename, 'w', encoding='utf-8') as f:
            json.dump({
                "collection": self.prompt_collection,
                "eras": self.era_classifications,
                "metadata": {
                    "export_date": datetime.now().isoformat(),
                    "total_prompts": len(self.prompt_collection),
                    "description": "Complete collection of Prompt Off competition winners (2006-2056)"
                }
            }, f, indent=2, ensure_ascii=False)

        return filename


# Convenience functions
def get_prompt_off_winner(year: int) -> Dict[str, Any]:
    """Get the winning prompt for a specific year."""
    masterworks = PromptOffMasterworks()
    return masterworks.get_prompt_by_year(year)

def list_all_categories() -> List[str]:
    """List all unique prompt categories."""
    masterworks = PromptOffMasterworks()
    return list(set(prompt.get("category") for prompt in masterworks.prompt_collection.values()))

def find_prompts_by_impact(impact_keyword: str) -> List[Dict[str, Any]]:
    """Find prompts by their historical impact."""
    masterworks = PromptOffMasterworks()
    return [prompt for prompt in masterworks.prompt_collection.values()
            if impact_keyword.lower() in prompt.get("impact", "").lower()]


if __name__ == "__main__":
    # Example usage
    masterworks = PromptOffMasterworks()

    print(f"Total prompts: {masterworks.get_collection_stats()['total_prompts']}")
    print("\nRandom prompt:")
    random_prompt = masterworks.get_random_prompt()
    print(f"{random_prompt['year']}: {random_prompt['title']}")
    print(f"Category: {random_prompt['category']}")
    print(f"Impact: {random_prompt['impact']}")

    print("\nPrompts by category 'Creativity':")
    creativity_prompts = masterworks.get_prompts_by_category("Creativity")
    for prompt in creativity_prompts:
        print(f"- {prompt['year']}: {prompt['title']}")

